{
  "name": "@diegofrayo/website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "no-opt-install": "yarn install --ignore-optional",
    "postinstall": "node --experimental-json-modules scripts/metadata.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"{src,scripts,public}/**/*.{js,ts,tsx,json,css,xml,mjs.md,mdx}\"",
    "husky:install": "cd ../../ && husky install packages/website/.husky",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "scripts:seo-files": "node --experimental-json-modules scripts/rss.mjs && node --experimental-json-modules scripts/sitemap.mjs",
    "scripts:static-files": "node --experimental-json-modules scripts/metadata.mjs && node --experimental-json-modules scripts/movies-and-books/script.mjs && node --experimental-json-modules scripts/list-of-chords.mjs",
    "scripts:contacts": "node --experimental-json-modules scripts/contacts/script.mjs",
    "scripts:transform-mdx-content": "node --experimental-json-modules scripts/mdx/script.mjs"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.0",
    "@splitbee/web": "^0.2.4",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
