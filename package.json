    "i18next": "^20.3.5",
    "next": "latest",
    "next-mdx-remote": "^4.0.0-alpha.2",
    "next-pwa": "^5.4.4",
    "next-themes": "^0.0.10",
    "nprogress": "^0.2.0",
    "prism-react-renderer": "latest",
    "react": "^17.0.2",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-i18next": "^11.11.4",
    "react-query": "^3.5.16",
