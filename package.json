{
	"name": "@diegofrayo/website",
	"version": "4.0.0",
	"private": true,
	"scripts": {
		"===== DEV =====": "",
		"dev": "yarn scripts:env-vars && next dev",
		"build:ts": "tsc --noEmit",
		"husky:install": "husky install ./.husky",
		"===== PROD =====": "",
		"no-opt-install": "yarn install --ignore-optional",
		"build": "next build",
		"start": "next start",
		"export": "next build && next export",
		"postinstall": "yarn scripts:rss",
		"===== FORMATTING =====": "",
		"lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"lint:no-warnings": "NO_LINT_WARNINGS=true eslint \"src/**/*.{ts,tsx}\"",
		"format": "prettier --write \"./**/*.{ts,tsx,json,css,mjs,js}\"",
		"===== STORYBOOK =====": "",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"===== SCRIPTS =====": "",
		"scripts:all": "yarn scripts:env-vars && yarn scripts:chords && yarn scripts:rss && yarn scripts:sitemap && yarn scripts:styles",
		"scripts:chords": "node --experimental-json-modules scripts/chords.mjs",
		"scripts:rss": "node --experimental-json-modules scripts/rss.mjs",
		"scripts:sitemap": "node --experimental-json-modules scripts/sitemap.mjs",
		"scripts:styles": "node scripts/styles/css-vars.js",
		"===== AUX/DEV =====": "",
		"scripts:commit": "sh ./.husky/generic-commit",
		"scripts:env-vars": "node --experimental-json-modules scripts/env-vars.mjs",
		"scripts:code-stats": "tokei ./src",
		"scripts:code-complexity": "npx code-complexity . --filter \"./src/**/*.tsx\" --limit 10 --sort complexity"
	},
	"dependencies": {
		"@heroicons/react": "^2.0.4",
		"@mdx-js/react": "^2.0.0",
		"@radix-ui/react-tabs": "^1.0.1",
		"@reduxjs/toolkit": "^1.6.1",
		"@splitbee/web": "^0.2.4",
		"auto-bind": "^5.0.1",
		"axios": "^0.21.1",
		"classnames": "^2.2.6",
		"crypto-js": "^4.0.0",
		"dayjs": "^1.9.7",
		"dom-to-image": "^2.6.0",
		"hoist-non-react-statics": "^3.3.2",
		"i18next": "^20.3.5",
		"next": "^12.3.0",
		"next-mdx-remote": "^4.0.0-alpha.2",
		"next-pwa": "^5.6.0",
		"next-themes": "^0.0.10",
		"nprogress": "^0.2.0",
		"prism-react-renderer": "^1.3.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-error-boundary": "^3.1.1",
		"react-i18next": "^11.11.4",
		"react-markdown": "^8.0.3",
		"react-query": "^3.5.16",
		"react-redux": "^7.2.4",
		"react-string-replace": "^0.4.4",
		"react-toastify": "^7.0.3",
		"swiper": "^8.1.0",
		"tilg": "^0.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@mdx-js/loader": "^2.1.3",
		"@next/mdx": "^12.2.5",
		"@types/classnames": "^2.2.11",
		"@types/crypto-js": "^4.1.1",
		"@types/dom-to-image": "^2.6.4",
		"@types/node": "^14.14.10",
